{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api?path=/$1"
    },
    {
      "source": "/v1/(.*)",
      "destination": "/api?path=/v1/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api?path=/$1"
    }
  ]
}
